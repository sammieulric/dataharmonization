@model SCBHarmonization.ViewModels.tbl_TransactionViewModel

@{
    ViewBag.Title = "GetTransactionTypeList";
    Layout = "~/Views/Shared/_Layoutc.cshtml";
}

<style>
    tr:hover {
        background-color: #f5f5f5;
    }

    tr:nth-child(even) {
        background-color: #f2f2f2;
    }

    th {
        background-color: #202A44;
        color: white;
    }
</style>
<h4>GetTransactionType Response : @ViewBag.Message</h4>

<div>
    @Html.ActionLink("Home", "Index", "Home")
</div>

<br />
<br />

@using SCBHarmonization.NibssModels
@{ 
    var transactiontype = ViewData["TransactionType"] as TransactionType;
}
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Code</th>
            <th scope="col">TransactionGroupName</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row"></th>
            <td>@transactiontype.Name</td>
            <td>@transactiontype.Code</td>
            <td>@transactiontype.TransactionGroupName</td>
        </tr>
    </tbody>
</table>